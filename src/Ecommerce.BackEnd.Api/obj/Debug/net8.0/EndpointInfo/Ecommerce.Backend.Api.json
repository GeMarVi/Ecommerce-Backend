                "type": "object",
                "properties": {
                  "id": {
                    "maxLength": 40,
                    "pattern": "^[0-9a-fA-F]{8}(-[0-9a-fA-F]{4}){3}-[0-9a-fA-F]{12}$",
                    "type": "string"
                  },
                  "code": {
                    "maxLength": 6,
                    "minLength": 0,
                    "type": "string"
                  }
