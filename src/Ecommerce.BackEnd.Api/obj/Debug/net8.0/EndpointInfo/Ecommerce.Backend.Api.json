{
  "openapi": "3.0.1",
  "info": {
    "title": "Sniker_Store",
    "version": "v1"
  },
  "paths": {
    "/api/User/v1/user-register": {
      "post": {
        "tags": [
          "User"
        ],
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "required": [
                  "Email",
                  "Password"
                ],
                "type": "object",
                "properties": {
                  "Email": {
                    "type": "string",
                    "format": "email"
                  },
                  "Password": {
                    "pattern": "^(?=.*[A-Z])(?=.*\\d)(?=.*\\W)(?=.*[a-z]).{6,}$",
                    "type": "string"
                  }
                }
              },
              "encoding": {
                "Email": {
                  "style": "form"
                },
                "Password": {
                  "style": "form"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "securitySchemes": {
      "Bearer": {
        "type": "apiKey",
        "description": "JWT Authorization header using the Bearer scheme. \r\n                        \\r\\n\\r\\n Enter prefix (Bearer), space, and then your token. \r\n                        Example: 'Bearer 1231233kjsdlkajdksad'",
        "name": "Authorization",
        "in": "header"
      }
    }
  },
  "security": [
    {
      "Bearer": [ ]
    }
  ]
}