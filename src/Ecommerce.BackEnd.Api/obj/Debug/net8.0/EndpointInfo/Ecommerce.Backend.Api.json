        ],
        "type": "object",
        "properties": {
          "token": {
            "maxLength": 1000,
            "minLength": 1,
            "type": "string"
          },
          "refreshToken": {
            "maxLength": 40,
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      }
    },
    "securitySchemes": {
      "Bearer": {
        "type": "apiKey",
        "description": "JWT Authorization header using the Bearer scheme. \r\n                        \\r\\n\\r\\n Enter prefix (Bearer), space, and then your token. \r\n                        Example: 'Bearer 1231233kjsdlkajdksad'",
        "name": "Authorization",
